{
  "name": "@crypticdot/defituna-client",
  "description": "Typescript client to interact with DefiTuna's on-chain program.",
  "version": "3.4.7",
  "private": false,
  "license": "SEE LICENSE IN LICENSE",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "node ./codama.mjs && tsup src/index.ts --format cjs,esm --dts",
    "test": "RUST_LOG=error vitest run tests --no-cache",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "eslint": "eslint \"src/**/*.ts\"",
    "eslint:fix": "eslint \"src/**/*.ts\" --fix",
    "type-check": "tsc -p .",
    "lint": "pnpm eslint && pnpm type-check",
    "clean": "rimraf dist src/generated"
  },
  "dependencies": {
    "@crypticdot/defituna-core": "workspace:*",
    "@orca-so/whirlpools-core": "^2.0.0",
    "@orca-so/whirlpools-client": "^4.0.1",
    "@orca-so/whirlpools": "^4.0.0",
    "@crypticdot/fusionamm-core": "^1.0.76",
    "@crypticdot/fusionamm-client": "^1.0.76",
    "@crypticdot/fusionamm-sdk": "^1.0.76",
    "@solana/kit": "^2.1.0",
    "@solana/sysvars": "^2.1.0",
    "@solana-program/compute-budget": "^0.7.0",
    "@solana-program/system": "^0.7.0",
    "@solana-program/token": "^0.5.1",
    "@solana-program/token-2022": "^0.4.2",
    "@solana-program/memo": "^0.7.0",
    "@solana-program/address-lookup-table": "^0.7.0"
  },
  "devDependencies": {
    "@crypticdot/defituna-program": "workspace:*",
    "@crypticdot/jupiter-solana-client": "^1.0.0",
    "@crypticdot/typescript-config": "^1.0.0",
    "@crypticdot/eslint-config": "^1.0.0",
    "@crypticdot/prettier-config": "^1.0.0",
    "@codama/nodes": "^1.2.13",
    "@codama/renderers-js": "^1.2.14",
    "@codama/visitors-core": "^1.2.13 ",
    "@codama/nodes-from-anchor": "^1.1.14",
    "@types/node": "22.9.0",
    "codama": "^1.2.14",
    "dotenv": "^16.4.7",
    "eslint": "^8.57.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "tsx": "^4.19.3",
    "typescript": "5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "rimraf": "^6.0.1",
    "vitest": "^3.1.1",
    "solana-bankrun": "^0.4.0"
  }
}